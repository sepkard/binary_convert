#include <stdio.h>
#include <stdlib.h>


int main(int argc, char *argv[]) 
{
	
	int x;
	printf("Ikilik sistemdeki sayiyi giriniz:");
	scanf("%d",&x);
	int a,b,c,d,e,g,f;
	
	a=x%10;
	if(a!=0)
	{
		a=1;
	}
	else
	a=0;
	b=x%100;
	if(b>=10)
	{
		b=2;		
	}
	else
	b=0;
	c=x%1000;
	if(c>=100)
	{
		c=4;
	}
	else
	c=0;
	d=x%10000;
	if(d>=1000)
	{
		d=8;
	}
	else
	d=0;
	e=x%100000;
	if(e>=10000)
	{
		e=16;
	}
	else
	e=0;
	g=x%1000000;
	if(g>=100000)
	{
		g=32;
	}
	else
	g=0;
	f=a+b+c+d+e+g;
    printf("%d",f);
	
	return 0;
}
